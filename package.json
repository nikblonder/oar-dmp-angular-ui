{
    "name": "oar-dmp",
    "workspaces": [
        "lib",
        "oar-dmp"
    ],
    "scripts": {
        "build-lib": "npm run build oarng --prefix ./lib",
        "build-oardmp": "npm run build dmp_ui2 --prefix ./oar-dmp",
        "build-all": "npm run build oarng --prefix ./lib && npm run build oar-dmp --prefix ./oar-dmp",
